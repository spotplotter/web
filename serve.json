{
  "public": "build",
  "rewrites": [{ "source": "**", "destination": "/index.html" }],
  "cleanUrls": true,
  "headers": [
    {
      "source": "**/*.@(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot|otf|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
